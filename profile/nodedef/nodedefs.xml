<nodeDefs>
  <nodeDef id="RainMachine" nls="ctl">
    <sts>
      <st id="ST" editor="bool" />
      <st id="GV0" editor="STATE" />
      <st id="GV3" editor="bool" />
      <st id="GV4" editor="LOGLEVEL" />
    </sts>
    <cmds>
        <sends>
            <cmd id = "LOG_LEVEL"/>
        </sends>
        <accepts>
          <cmd id="LOG_LEVEL" >
              <p id= "" editor="LOGLEVEL" init="GV4" />
          </cmd>
          <cmd id="WINTER" >
              <p id= "" editor="bool" init="GV3" />
          </cmd>
          <cmd id="QUERY"/>
          <cmd id="DISCOVER" />
          <cmd id="REMOVE_NOTICES_ALL" />
          <cmd id="UPDATE_PROFILE" />
        </accepts>
    </cmds>
  </nodeDef>

  <nodeDef id="zone" nls="RMZ">
    <sts >
      <st id="ST" editor="I_ZONESTATUS" />
      <st id="GV3" editor="I_MINUTES" />
      <st id="GV4" editor="I_SECONDS" />
      <st id="GV5" editor="bool" />
     </sts>
    <cmds>
        <sends>
          <cmd id="RUN" />
        </sends>
        <accepts>
            <cmd id="RUN" >
              <p id= "" editor="RUNTIME"/>
            </cmd>
            <cmd id = "QUERY" />
            <cmd id = "STOP" />
        </accepts>
    </cmds>
  </nodeDef>

  <nodeDef id="program" nls="RMPROG">
    <sts >
      <st id="ST" editor="I_ZONESTATUS" />
      <st id="GV3" editor="WEEKDAY" />
     </sts>
    <cmds>
        <accepts>
            <cmd id = "RUN" />
            <cmd id = "QUERY" />
            <cmd id = "STOP" />
        </accepts>
    </cmds>
  </nodeDef>

   <nodeDef id="precip" nls="RMPRECIP">
    <sts >
        <st id="ST" editor="I_MM" />
        <st id="GV0" editor="I_MM" />
        <st id="GV1" editor="I_MM" />
        <st id="GV2" editor="I_MM"/>
     </sts>
    <cmds>
        <accepts>
            <cmd id = "QUERY" />
        </accepts>
    </cmds>
  </nodeDef>

 <nodeDef id="restrict" nls="RMRESTRICT">
    <sts >
        <st id="ST" editor="I_STATE" />
        <st id="GV0" editor="I_MINUTES" />
        <st id="GV1" editor="I_STATE" />
        <st id="GV2" editor="bool"/>
        <st id="GV3" editor="bool"/>
        <st id="GV4" editor="bool"/>
     </sts>
    <cmds>
        <sends>
            <cmd id = "RAIN_DELAY"/>
        </sends>
        <accepts>
          <cmd id="RAIN_DELAY">
              <p id= "" editor="I_DAYS" />
          </cmd>
          <cmd id = "QUERY" />
        </accepts>
    </cmds>
  </nodeDef>
 </nodeDefs>